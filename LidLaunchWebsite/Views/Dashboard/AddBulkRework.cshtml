<div id="addBulkReworkPopup">
    <div id="addBulkReworkPopupInner" style="padding:20px;">
        <span onclick="$('#addBulkReworkPopup').hide();" class="closeButton" style="display:block; top:5px;right:5px; float:right;">X</span>
        <div><span id="bulkOrderBlankName">@Model.bulkOrderBlankName</span><input type="text" id="bulkReworkQuantity" value="@Model.quantity"/></div>     
        <div style="margin:20px;"><input id="bulkReworkNote" type="text" placeholder="note..." value="@Model.note"/></div>
        <div style="margin:20px;">
            <select id="selReworkStatus">
                <option value="In Progress">In Progress</option>
                <option value="Complete">Complete</option>
            </select>
        </div>
        <div id="saveNote"><input type="button" value="Save Rework" class="smallButton" onclick="saveBulkRework('@Model.bulkOrderBatchId','@Model.bulkOrderItemId', '@Model.bulkOrderBlankName',  '@Model.parentBulkOrderId', '@Model.parentBulkOrderBatchId', '@Model.bulkReworkId', '@Model.shipping')" /></div>
    </div>
</div>