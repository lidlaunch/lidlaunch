    <div id="flexFit6277Region" class="clear">
        <div class="regionHeader" onclick="$('#6277Inner').toggle();">
            <h2><img src="~/Images/FlexFit6277/hat_01.jpg" /> FlexFit 6277</h2>

        </div>
        <div id="6277Inner" class="regionInner hidden">
            <table class="inventoryTable">
                <tr style="font-weight:bold;">
                    <td></td>
                    <td></td>
                    <td>S/M</td>
                    <td>L/XL</td>
                    <td>XL/XXL</td>
                </tr>
                @{
                    var count = 0;
                    foreach (LidLaunchWebsite.Models.MasterBulkOrderItem item in Model.FlexFit6277Items)
                    {
                        <tr class="inventoryRow">
                            <td><img src='~/Images/@item.ThumbnailpreviewImagePath' /></td>
                            <td><span class="hidden itemId">@item.Id</span><b>@item.ItemColor</b></td>
                            @if (item.SM)
                            {

                                <td>
                                    <span class="incrementButton"><input type="button" class="smallButton" value="+" onclick="incrementTotal(this)" /></span><br />
                                    <span class="valueHolder"><input type="number" class="txtValue" value="@item.SMInternalStock" /><span class="hidden sizeValue">SM</span></span><br />
                                    <span class="decrementButton"><input type="button" class="smallButton" value="-" onclick="decrementTotal(this)" /></span>
                                </td>
                            }
                            else
                            {
                                <td></td>
                            }
                            @if (item.LXL)
                            {
                                <td>
                                    <span class="incrementButton"><input type="button" class="smallButton" value="+" onclick="incrementTotal(this)" /></span><br />
                                    <span class="valueHolder"><input type="number" class="txtValue" value="@item.LXLInternalStock" /><span class="hidden sizeValue">LXL</span></span><br />
                                    <span class="decrementButton"><input type="button" class="smallButton" value="-" onclick="decrementTotal(this)" /></span>
                                </td>
                            }
                            else
                            {
                                <td></td>
                            }
                            @if (item.XLXXL)
                            {
                                <td>
                                    <span class="incrementButton"><input type="button" class="smallButton" value="+" onclick="incrementTotal(this)" /></span><br />
                                    <span class="valueHolder"><input type="number" class="txtValue" value="@item.XLXXLInternalStock" /><span class="hidden sizeValue">XLXXL</span></span><br />
                                    <span class="decrementButton"><input type="button" class="smallButton" value="-" onclick="decrementTotal(this)" /></span>

                                </td>
                            }
                            else
                            {
                                <td></td>
                            }
                        </tr>
                        if (count >= (Model.FlexFit6277Items.Count / 4))
                        {
                            count = 0;
                            @Html.Raw("</table><table class='inventoryTable'><tr style='font-weight:bold;'><td></td><td></td><td>S/M</td><td>L/XL</td><td>XL/XXL</td>")
                        }
                        else
                        {
                            count++;
                        }
