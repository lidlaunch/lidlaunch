    function incrementTotal(that) {
        var currentTotal = $(that).closest('td').find('.txtValue').val();
        var itemId = $(that).closest('tr').find('.itemId').text();
        var sizeValue = $(that).closest('td').find('.sizeValue').text();
        currentTotal++;

        showLoading();
        $.ajax({
            type: "POST",
            url: '/Inventory/UpdateMasterBulkOrderItemInternalInventory',
            contentType: false,
            processData: false,
            data: JSON.stringify({
                id: itemId, inventoryValue: currentTotal, sizeValue: sizeValue
            }),
            contentType: "application/json",
            success: function (result) {
                if (result == "") {
                    //do nothing
                    displayPopupNotification('error.', 'error', false);
                } else {
                    hideLoading();
                    $(that).closest('td').find('.txtValue').val(currentTotal);
                }
            },
            error: function (xhr, status, p3, p4) {
                displayPopupNotification('Error.', 'error', false);
            }
        });

    }
