<style type="text/css">
    .check {
        width: 40px;
        height: 40px;
    }

    .inventoryTable {
        display: inline-table;
        width: 380px;
        border-right: 5px solid #eee;
        border-collapse: collapse;
    }

    .regionInner {
        max-width: 100%;
        overflow: scroll;
        width: 100%;
    }
    .regionHeader {
        cursor:pointer;
    }
    .regionHeader img {
        height: 30px;
    }
    .incrementButton input[type="button"] {
        width: 60px;
        font-weight: bold;
        font-size: 18px;
    }
    .decrementButton input[type="button"] {
        width: 60px;
        font-weight: bold;
        font-size: 18px;
    }
    .inventoryRow {
        border-bottom: 5px solid #eee;
    }
    .decrementButton {
        display: inline-block;
    }

    .valueHolder {
        display: inline-block;
    }

    .txtValue {
        width: 50px;
    }

    .incrementButton {
        display: inline-block;
    }
    .inventoryRow img {
        height: 60px;
    }
    .inventoryRow b {
        max-width: 65px;
        word-break: break-all;
        display: block;
        font-size: 14px;
    }
</style>

<div class="centerInner" style="padding:20px">
